{
  "name": "@bigbinary/babel-preset-neeto",
  "version": "1.0.1",
  "description": "A babel preset to transform short code to boilerplate at compile time",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "prepare": "husky install"
